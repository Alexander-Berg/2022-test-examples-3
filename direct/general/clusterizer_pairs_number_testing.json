{
  "tableName": "clusterizer_pairs_number_testing",
  "splits": {
    "host": "host"
  },
  "metrics": [
    {
      "metricName": "ir.models-clusterizer.testing.pairs_number.${host}.random_pairs_number",
      "period": "ONE_MIN",
      "type": "SIMPLE",
      "metricField": "sum(random_pairs_number)"
    },
    {
      "metricName": "ir.models-clusterizer.testing.pairs_number.${host}.doubtful_pairs_number",
      "period": "ONE_MIN",
      "type": "SIMPLE",
      "metricField": "sum(doubtful_pairs_number)"
    },
    {
      "metricName": "ir.models-clusterizer.testing.pairs_number.${host}.bad_close_pairs_number",
      "period": "ONE_MIN",
      "type": "SIMPLE",
      "metricField": "sum(bad_close_pairs_number)"
    }
  ]
}