{
  "logHosts": "test*",
  "logPath": "test.log",
  "clickhouseTable": "test",
  "parser": {
    "dateFormat": "yyyy-MM-dd'T'HH:mm:ss.SSSX",
    "separator": "\t",
    "tskvMatch": {},
    "useDefaultOnEmpty": true,

    "columns": {
      "host": {
        "type": "String"
      },
      "method": {
        "type": "String",
        "default": "DEFAULT_VALUE"
      },
      "client_ip": {
        "type": "String",
        "default": "DEFAULT_VALUE"
      },
      "http_code": {
        "type": "UInt16",
        "default": "200"
      },
      "resptime_ms": {
        "type": "Int32",
        "default": "0"
      }
    }
  }
}
