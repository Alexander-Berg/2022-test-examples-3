{
    "meta": {
        "name": "yandex-du-launch-sandbox-tests-in-release",
        "maintainer": "Yury Kabargin <yukaba@yandex-team.ru>",
        "description": "Launcher for different tests in Sandbox in releases",
        "version": "1.{revision}-1"
    },
    "build": {
        "targets": [
            "direct/infra/dt-launch-sandbox-tests-in-release"
        ]
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "direct/infra/dt-launch-sandbox-tests-in-release/dt-launch-sandbox-tests-in-release"
            },
            "destination": {
                "path": "/usr/local/bin/dt-launch-sandbox-tests-in-release"
            }
        }
    ]
}
