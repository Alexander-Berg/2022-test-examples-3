{
  "vertical": "web",
  "search-engine": "YANDEX",
  "search-engine-type": "yandex-web-test",
  "user-agent-type": "DESKTOP_YANDEX_BROWSER",
  "parser-config": {
    "type": "java-parser-config",  
    "classname": "ru.yandex.qe.scraper.parsers.concrete.yandex.html.yandex.web.YandexWebParser"
  },
  "request-preparer": {
    "path": "/search",
    "query-parameter": "text",
    "page-parameter": "lr",
    "normalization-strategy": "FORCE_NO_WWW",
    "required-parameters": [
      "query-text"
    ],
    "optional-parameters": [
      "allow-parent-region-id",
      "region-id",
      "additional-cgi",
      "additional-cookies",
      "additional-headers"
    ]
  }
}
