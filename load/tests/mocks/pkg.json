{
    "meta": {
        "name": "mocks-yandextank-testing",
        "description": "Mocks for yandex-tank testing",
        "version": "0.4",
        "maintainer": "load@yandex-team.ru",
        "homepage": "https://wiki.yandex-team.ru/load/"
    },
    "build": {
        "targets": [
            "load/projects/yandex-tank-internal-pkg/tests/mocks"
        ]
    },
    "data": [
        {
            "source": {
                "type": "ARCADIA",
                "path": "load/projects/yandex-tank-internal-pkg/tests/mocks/configs",
                "files": ["*"]
            },
            "destination": {
                "path": "/configs/"
            }
        },
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        }
    ]
}


