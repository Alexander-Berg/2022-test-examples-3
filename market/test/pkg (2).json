{
  "meta": {
    "name": "yandex-market-kraken-config-test",
    "maintainer": "Maksim Kupriyanov <maxk@yandex-team.ru>",
    "description": "Configuration files for yandex-market-kraken",
    "version": "0.0-{revision}",
    "depends": ["yandex-market-kraken"],
    "homepage": "https://a.yandex-team.ru/arc/trunk/arcadia/market/sre/conf/kraken",
    "noconffiles_all": false
  },
  "data": [
    {
      "source": {
        "type": "ARCADIA",
        "path": "market/sre/conf/kraken/common/etc",
        "files": ["*"]
      },
      "destination": {
        "path": "/etc/"
      }
    },
    {
      "source": {
        "type": "RELATIVE",
        "path": "etc",
        "files": ["*"]
      },
      "destination": {
        "path": "/etc/"
      }
    }
  ]
}
