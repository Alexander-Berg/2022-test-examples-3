<?xml version="1.0" encoding="UTF-8"?>
<m:config xmlns:c="urn:jmf:common:1.0"
          xmlns:m="urn:jmf:metaclass:config:1.0"
          xmlns:a="urn:jmf:attribute:type:default:config:1.0"
          xmlns:s="urn:jmf:module:search:extension:config:1.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
>
    <m:metaclass>
        <m:fqn>smpl</m:fqn>
        <m:parent>bo</m:parent>
        <m:title>
            <c:value>Объект для тестирования UI</c:value>
        </m:title>
        <m:logics>
            <m:logic>withSearch</m:logic>
        </m:logics>
        <m:attributes>
            <m:attribute>
                <m:code>attr</m:code>
                <m:title>
                    <c:value>Прямой атрибут</c:value>
                </m:title>
                <m:type xsi:type="a:string"/>
            </m:attribute>
        </m:attributes>

        <m:extensions>
            <m:extension xsi:type="s:search" globalSearchEnabled="true">
                <s:search>
                    <s:attribute code="title"/>
                </s:search>
            </m:extension>
        </m:extensions>
    </m:metaclass>

</m:config>
