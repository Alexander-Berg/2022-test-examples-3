<?xml version="1.0" encoding="UTF-8"?>
<m:config xmlns:c="urn:jmf:common:1.0"
          xmlns:m="urn:jmf:metaclass:config:1.0"
          xmlns:a="urn:jmf:attribute:type:default:config:1.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
>
    <m:metaclassOverride>
        <m:fqn>configuration</m:fqn>
        <m:attributes>
            <m:attribute>
                <m:code>integerProperty</m:code>
                <m:title>
                    <c:value>Пример свойства конфигуации типа "Число"</c:value>
                </m:title>
                <m:type xsi:type="a:integer"/>
                <m:default>${test.defaultIntegerPropertyValue}</m:default>
            </m:attribute>
            <m:attribute>
                <m:code>decimalProperty</m:code>
                <m:title>
                    <c:value>Пример свойства конфигуации типа "Decimal"</c:value>
                </m:title>
                <m:type xsi:type="a:decimal"/>
                <m:default>${test.defaultDecimalPropertyValue}</m:default>
            </m:attribute>
            <m:attribute>
                <m:code>stringProperty</m:code>
                <m:title>
                    <c:value>Пример свойства конфигуации типа "Строка"</c:value>
                </m:title>
                <m:type xsi:type="a:string"/>
                <m:default>${test.defaultStringPropertyValue}</m:default>
            </m:attribute>
            <m:attribute>
                <m:code>booleanProperty</m:code>
                <m:title>
                    <c:value>Пример свойства конфигуации типа "Boolean"</c:value>
                </m:title>
                <m:type xsi:type="a:boolean"/>
            </m:attribute>
        </m:attributes>
    </m:metaclassOverride>

</m:config>
