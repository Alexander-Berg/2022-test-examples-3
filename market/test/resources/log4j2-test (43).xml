<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN" strict="true">
    <Appenders>
        <Console name="stdout">
            <PatternLayout pattern="%date{HH:mm:ss.SSS} | %highlight{%.-1level} | %thread, %logger{36} : %message%n%throwable"/>
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="ru.yandex.market" level="info"/>
        <Root level="warn">
            <AppenderRef ref="stdout"/>
        </Root>
    </Loggers>
</Configuration>
