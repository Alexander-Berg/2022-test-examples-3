<?xml version="1.0"?>
<dataset>

    <PUTAWAYZONE PUTAWAYZONE="RACK"/>
    <PUTAWAYZONE PUTAWAYZONE="GATE_A_ZN"/>
    <PUTAWAYZONE PUTAWAYZONE="GATE_B_ZN"/>

    <NSQLCONFIG CONFIGKEY="YM_SHIP_ID_REGEX" NSQLVALUE="^DRP\d+$" />
    <NSQLCONFIG CONFIGKEY="YM_MORNING_CUTOFF_TIME" NSQLVALUE="12:00" />
    <NSQLCONFIG CONFIGKEY="SHIPPING_LOAD_DROP_DELAY" NSQLVALUE="11" />
    <NSQLCONFIG CONFIGKEY="SHIPPING_LOAD_WITHD_DROP_DELAY" NSQLVALUE="22" />

    <LOC LOC="UNKNOWN" STATUS="OK" PUTAWAYZONE="RACK"      LOCATIONTYPE="OTHER"/>
    <LOC LOC="PACK"    STATUS="OK" PUTAWAYZONE="RACK"      LOCATIONTYPE="OTHER"/>
    <!--  Ворота  -->
    <LOC LOC="DOOR_A"  STATUS="OK" PUTAWAYZONE="GATE_A_ZN" LOCATIONTYPE="DOOR_OUT"/>
    <LOC LOC="DOOR_B"  STATUS="OK" PUTAWAYZONE="GATE_B_ZN" LOCATIONTYPE="DOOR_OUT"/>
    <!--  Ячейки для дропок с обычными заказами  -->
    <LOC LOC="A_STD_1" STATUS="OK" PUTAWAYZONE="GATE_A_ZN" LOCATIONTYPE="SHIP_STD"/>
    <LOC LOC="A_STD_2" STATUS="OK" PUTAWAYZONE="GATE_A_ZN" LOCATIONTYPE="SHIP_STD"/>
    <LOC LOC="A_STD_3" STATUS="OK" PUTAWAYZONE="GATE_A_ZN" LOCATIONTYPE="SHIP_STD"/>
    <LOC LOC="B_STD_1" STATUS="OK" PUTAWAYZONE="GATE_B_ZN" LOCATIONTYPE="SHIP_STD"/>
    <!--  Ячейки для дропок с изъятиями  -->
    <LOC LOC="A_WITHD_1" STATUS="OK" PUTAWAYZONE="GATE_A_ZN" LOCATIONTYPE="SHIP_WITHD"/>
    <LOC LOC="A_WITHD_2" STATUS="OK" PUTAWAYZONE="GATE_A_ZN" LOCATIONTYPE="SHIP_WITHD"/>
    <LOC LOC="B_WITHD_1" STATUS="OK" PUTAWAYZONE="GATE_B_ZN" LOCATIONTYPE="SHIP_WITHD"/>
    <!--  Буферные ячейки для дропок с обычными заказами  -->
    <LOC LOC="A_BUF_1"  STATUS="OK" PUTAWAYZONE="GATE_A_ZN" LOCATIONTYPE="SHIP_BUF"/>
    <LOC LOC="A_BUF_2"  STATUS="OK" PUTAWAYZONE="GATE_A_ZN" LOCATIONTYPE="SHIP_BUF"/>
    <LOC LOC="B_BUF_1"  STATUS="OK" PUTAWAYZONE="GATE_B_ZN" LOCATIONTYPE="SHIP_BUF"/>
    <!--  Буферная ячейка для изъятий. Выдется даже если в баллансах на ней есть другие дропки  -->
    <LOC LOC="WITH_BUF_1" STATUS="OK" PUTAWAYZONE="GATE_A_ZN" LOCATIONTYPE="SHIP_WTBUF"/>

    <PACK PACKKEY="STD" PACKDESCR="STD"/>

    <SKU SKU="ROV0001" STORERKEY="1234"/>
    <SKU SKU="ROV0002" STORERKEY="1234"/>
    <SKU SKU="ROV0003" STORERKEY="1234"/>

    <DOOR_CARRIERCODE DOOR="DOOR_A" CARRIERCODE="CARRIER-01" ADDWHO="SETUP"/>
    <DOOR_CARRIERCODE DOOR="DOOR_B" CARRIERCODE="CARRIER-02" ADDWHO="SETUP"/>

    <STORER STORERKEY="CARRIER-01" COMPANY="Test carrier 1" TYPE="3"/>
    <STORER STORERKEY="CARRIER-02" COMPANY="Test carrier 2" TYPE="3"/>

    <USERACTIVITY
        ASSIGNMENTNUMBER="11111"
        STATUS="1"
        ACTYPE="1"
        TYPE="LUNCH"
        USERID="TEST"
    />
