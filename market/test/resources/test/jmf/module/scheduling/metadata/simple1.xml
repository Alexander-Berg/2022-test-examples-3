<?xml version="1.0" encoding="UTF-8"?>
<m:config xmlns:c="urn:jmf:common:1.0"
          xmlns:m="urn:jmf:metaclass:config:1.0"
          xmlns:a="urn:jmf:attribute:type:default:config:1.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
>

    <m:metaclass>
        <m:fqn>simple1</m:fqn>
        <m:parent>systemEntity</m:parent>
        <m:title>
            <c:value>Простой объект для тестирования</c:value>
        </m:title>
        <m:logics>
            <m:logic>scheduled</m:logic>
        </m:logics>
        <m:attributes>
            <m:attribute>
                <m:code>enableFailedSchedulerStrategy</m:code>
                <m:title>
                    <c:value>Влючать ли тестовую ошибочную стратегию для планировщика</c:value>
                </m:title>
                <m:type xsi:type="a:boolean"/>
                <m:default>false</m:default>
            </m:attribute>
            <m:attribute>
                <m:code>enableSlowSchedulerStrategy</m:code>
                <m:title>
                    <c:value>Влючать ли тестовую медленную стратегию для планировщика</c:value>
                </m:title>
                <m:type xsi:type="a:boolean"/>
                <m:default>false</m:default>
            </m:attribute>
        </m:attributes>
    </m:metaclass>

</m:config>
