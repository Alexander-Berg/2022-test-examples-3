<?xml version="1.0" encoding="UTF-8"?>
<m:config xmlns:c="urn:jmf:common:1.0"
          xmlns:m="urn:jmf:metaclass:config:1.0">

    <m:metaclass typed="false">
        <m:fqn>simpleEntity</m:fqn>
        <m:title>
            <c:value>Простой объект для тестирования</c:value>
        </m:title>
        <m:logics>
            <m:logic>db</m:logic>
        </m:logics>
    </m:metaclass>

    <m:metaclass>
        <m:fqn>transientEntity</m:fqn>
        <m:title>
            <c:value>Простой персистентный объект для тестирования</c:value>
        </m:title>
        <m:interfaces>
            <m:interface>ru.yandex.market.jmf.trigger.test.TransientEntity</m:interface>
        </m:interfaces>
        <m:attributes>
            <m:attribute>
                <m:code>attrInt</m:code>
                <m:title>
                    <c:value>Атрибут число</c:value>
                </m:title>
                <m:type code="integer"/>
            </m:attribute>
            <m:attribute>
                <m:code>attrStr</m:code>
                <m:title>
                    <c:value>Атрибут строка</c:value>
                </m:title>
                <m:type code="string"/>
            </m:attribute>
        </m:attributes>
    </m:metaclass>

</m:config>