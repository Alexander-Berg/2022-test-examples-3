<?xml version="1.0" encoding="UTF-8"?>
<m:config xmlns:c="urn:jmf:common:1.0"
          xmlns:m="urn:jmf:metaclass:config:1.0"
          xmlns:a="urn:jmf:attribute:type:default:config:1.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
>
    <m:metaclass>
        <m:fqn>main</m:fqn>
        <m:parent>bo</m:parent>
        <m:title>
            <c:value>Метакласс, хранящийся на основном шарде</c:value>
        </m:title>
        <m:attributes>
            <m:attribute>
                <m:code>attr1</m:code>
                <m:title>
                    <c:value>String attribute</c:value>
                </m:title>
                <m:type xsi:type="a:string"/>
            </m:attribute>
            <m:attribute>
                <m:code>shardedObj</m:code>
                <m:title>
                    <c:value>Объект из шарда sharded</c:value>
                </m:title>
                <m:type xsi:type="a:object" fqn="sharded"/>
            </m:attribute>
        </m:attributes>
    </m:metaclass>
</m:config>
