<script>
var photos_chassic=new Array(3)
var photos_chassiclink=new Array(3)
var which_chassic=0

var photos_case=new Array(4)
var photos_caselink=new Array(4)
var which_case=0

function backward_case(){
	if (which_case>0){
		which_case--
	}else {
		which_case=photos_case.length-1
	}
	if(photos_case[which_case]){
		document.images.photos_caselider.src=photos_case[which_case]
	}else{
		which_case=0
		document.images.photos_caselider.src=photos_case[which_case]
	}
}

function backward_chassic(){
	if (which_chassic>0){
		which_chassic--
	}else {
		which_chassic=photos_chassic.length-1
	}
	if(photos_chassic[which_chassic]){
		document.images.photos_chassiclider.src=photos_chassic[which_chassic]
	}else{
		which_chassic=0
		document.images.photos_chassiclider.src=photos_chassic[which_chassic]
	}
}

function transport_case(){
	window.open(photos_caselink[which_case])
}

function transport_chassic(){
	window.open(photos_chassiclink[which_chassic])
}

function forward_case(){
	if (which_case<photos_case.length-1){
		which_case++
	}else{
		which_case=0
	}
	if(photos_case[which_case]){
		document.images.photos_caselider.src=photos_case[which_case]
	}else{
		which_case=0
		document.images.photos_caselider.src=photos_case[which_case]
	}
}

function forward_chassic(){
	if (which_chassic<photos_chassic.length-1){
		which_chassic++
	}else{
		which_chassic=0
	}
	if(photos_chassic[which_chassic]){
		document.images.photos_chassiclider.src=photos_chassic[which_chassic]
	}else{
		which_chassic=0
		document.images.photos_chassiclider.src=photos_chassic[which_chassic]
	}
}

function swap_img(case1,name,bcase1,case2,bcase2,case3,bcase3,case4,bcase4,chassic_pictire1,big_chassic_picture1,chassic_pictire2,big_chassic_picture2,chassic_pictire3,big_chassic_picture3) {
	
	document.getElementById("rzd_name").innerHTML = name
	
	
	photos_case[0]=case1
	photos_case[1]=case2
	photos_case[2]=case3
	photos_case[3]=case4
	
	photos_caselink[0]=bcase1
	photos_caselink[1]=bcase2
	photos_caselink[2]=bcase3
	photos_caselink[3]=bcase4
	
	
	which_case=0
	var preloaded_case_images=new Array()
			for (i=0;i<photos_case.length;i++){
			preloaded_case_images[i]=new Image()
			preloaded_case_images[i].src=photos_case[i]
			}
	 forward_case()
	 backward_case()
	
	
	
	photos_chassic[0]=chassic_pictire1
	photos_chassiclink[0]=big_chassic_picture1
	photos_chassic[1]=chassic_pictire2
	photos_chassiclink[1]=big_chassic_picture2
	photos_chassic[2]=chassic_pictire3
	photos_chassiclink[2]=big_chassic_picture3
	which_chassic=0
	var preloadedimages=new Array()
			for (i=0;i<photos_chassic.length;i++){
			preloadedimages[i]=new Image()
			preloadedimages[i].src=photos_chassic[i]
			}
	 forward_chassic()
	 backward_chassic()
}

</script>


