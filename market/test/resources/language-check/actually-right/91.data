<script language="JavaScript"
        type="text/javascript">
        function setActive2(o)
        {           
        	// Set the default selected tab
            if (!o)
            {       
            	var arr = new Array('features', 'specs', 'systems', 'components', 'accessories', 'userguides');
            	var i = 0;
            	for (i=0; i<arr.length; i++)
            	{
            		if (document.getElementById(arr[i]))
            		{
            			o = document.getElementById(arr[i]);
            			break;
            		}
            	}
            }   
            
            if (o)
            {      
            	// Reset all colors to default
            	changecolor('#D9D9D9');
            	
            	// Set the one selected
            	o.style.backgroundColor = '#293c64';
            	o.style.color = '#ffffff';
            	o.style.textDecoration = 'none';
            	
            	// Make the item visible
            	document.getElementById(o.id + '2').style.display = 'block';
            }
        }
        
        function changecolor(newcolor)
        {
            if(document.layers) // browser="NN4";
            {
                if (document.layers["features"])
                {
                    document.layers["features"].bgColor = newcolor;
                    document.layers["features"].color = '#5B5B5B';
                    document.layers["features"].style.textDecoration = 'underline';
                    document.layers["features2"].style.display = 'none';
                }
                
                if (document.layers["specs"])
                {
                    document.layers["specs"].bgColor = newcolor;
                    document.layers["specs"].color = '#5B5B5B';
                    document.layers["specs"].style.textDecoration = 'underline';
                    document.layers["specs2"].style.display = 'none';
                }
                
                if (document.layers["accessories"])
                {
                    document.layers["accessories"].bgColor = newcolor;
                    document.layers["accessories"].color = '#5B5B5B';
                    document.layers["accessories"].style.textDecoration = 'underline';
                    document.layers["accessories2"].style.display = 'none';
                }
                
                if (document.layers["systems"])
                {
                    document.layers["systems"].bgColor = newcolor;
                    document.layers["systems"].color = '#5B5B5B';
                    document.layers["systems"].style.textDecoration = 'underline';
                    document.layers["systems2"].style.display = 'none';
                }
                
                if (document.layers["components"])
                {
                    document.layers["components"].bgColor = newcolor;
                    document.layers["components"].color = '#5B5B5B';
                    document.layers["components"].style.textDecoration = 'underline';
                    document.layers["components2"].style.display = 'none';
                }
                
                if (document.layers["userguides"])
                {
                    document.layers["userguides"].bgColor = newcolor;
                    document.layers["userguides"].color = '#5B5B5B';
                    document.layers["userguides"].style.textDecoration = 'underline';
                    document.layers["userguides2"].style.display = 'none';
                }
            }
            if(document.all) // browser="IE";
            {               
                if (document.all.features)
                {
                    document.all.features.style.backgroundColor = newcolor;
                    document.all.features.style.color = '#5B5B5B';
                    document.all.features.style.textDecoration = 'underline';
                    document.all.features2.style.display = 'none';
                }
                
                if (document.all.specs)
                {
                    document.all.specs.style.backgroundColor = newcolor;
                    document.all.specs.style.color = '#5B5B5B';
                    document.all.specs.style.textDecoration = 'underline';
                    document.all.specs2.style.display = 'none';
                }
                
                if (document.all.accessories)
                {
                    document.all.accessories.style.backgroundColor = newcolor;
                    document.all.accessories.style.color = '#5B5B5B';
                    document.all.accessories.style.textDecoration = 'underline';
                    document.all.accessories2.style.display = 'none';
                }
                
                if (document.all.systems)
                {
                    document.all.systems.style.backgroundColor = newcolor;
                    document.all.systems.style.color = '#5B5B5B';
                    document.all.systems.style.textDecoration = 'underline';
                    document.all.systems2.style.display = 'none';
                }
                
                if (document.all.components)
                {
                    document.all.components.style.backgroundColor = newcolor;
                    document.all.components.style.color = '#5B5B5B';
                    document.all.components.style.textDecoration = 'underline';
                    document.all.components2.style.display = 'none'
                }   
                
                if (document.all.userguides)
