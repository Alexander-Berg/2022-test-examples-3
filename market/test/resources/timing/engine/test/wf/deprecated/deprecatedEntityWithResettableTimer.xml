<?xml version="1.0" encoding="UTF-8"?>
<w:config xmlns:c="urn:jmf:common:1.0"
          xmlns:w="urn:jmf:logic:wf:config:1.0">

    <w:workflow metaclass="deprecatedEntityWithResettableBackTimer">
        <w:statuses initial="st1">
            <w:status code="st1">
                <w:title>
                    <c:value>Status 1</c:value>
                </w:title>
            </w:status>
            <w:status code="st2">
                <w:title>
                    <c:value>Status 2</c:value>
                </w:title>
            </w:status>
            <w:status code="st3">
                <w:title>
                    <c:value>Status 3</c:value>
                </w:title>
            </w:status>
        </w:statuses>
        <w:transitions>
            <w:transition from="st1" to="st2"/>
            <w:transition from="st2" to="st3"/>
        </w:transitions>
    </w:workflow>

</w:config>
