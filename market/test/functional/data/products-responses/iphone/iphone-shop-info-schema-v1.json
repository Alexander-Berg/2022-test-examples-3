{
  "title": "iphone shopInfo schema v1",
  "type": "object",
  "required": ["shopName", "rating", "gradeTotal", "url"],
  "properties": {
    "shopName": {
      "type": "string"
    },
    "rating": {
      "type": "number",
      "minimum": 0,
      "maximum": 5
    },
    "gradeTotal": {
      "type": "number",
      "minimum": 0
    },
    "url": {
      "type": "string",
      "format": "uri"
    }
  }
}