{
  "title": "Categories product log schema",

  "type": "object",
  "required": [
    "category_id",
    "category_path",
    "category_formed_with",
    "root_category_id"
  ],
  "properties": {
    "category_id": {
      "type": "number"
    },
    "category_path": {
      "type": "string"
    },
    "category_formed_with": {
      "enum": ["classifier", "redirector"]
    },
    "redirector_category_name": {
      "type": "string"
    },
    "redirector_category_full_name": {
      "type": "string"
    }
  }
}