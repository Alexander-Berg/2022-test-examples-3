{
  "title": "Avia search check logs schema",
  "required": [
    "search_id",
    "type",
    "count",
    "min_price",
    "currency",
    "popup_flights_prices",
    "popup_flights_intents",
    "popup_flights_changes"
  ],
  "properties": {
    "search_id": {
      "type": "string"
    },
    "type": {
      "enum": ["check-search", "finish-search"]
    },
    "count": {
      "type": "number",
      "minimum": 0
    },
    "min_price": {
      "type": "number",
      "minimum": 0
    },
    "currency": {
      "enum": ["RUR", "RUB", "BYR", "BYN", "UAH", "KZT"]
    },
    "popup_flights_prices": {
      "type": "string"
    },
    "popup_flights_intents": {
      "type": "string"
    },
    "popup_flights_changes": {
      "type": "string"
    }
  }
}