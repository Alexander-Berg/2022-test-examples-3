{
  "name": "sovetnik-redir",
  "version": "1.6.0",
  "description": "Redirector for Sovetnik",
  "main": "server.js",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "test": "jest --forceExit",
    "test:coverage": "npm t -- --coverage",
    "test:watch": "npm t -- --watch",
    "test:ci": "npm t -- --config jest.config.ci.js",
    "lint": "eslint --quiet --ext .js .",
    "prettier": "prettier \"**/*.+(js|json)\"",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --list-different"
  },
  "dependencies": {
    "cookie-parser": "^1.3.2",
    "dotenv": "5.0.1",
    "express": "^4.13.1",
    "grunt-ssh": "^0.12.6",
    "luster": "^0.5.4",
    "luster-log-file": "^0.2.1",
    "luster-log-pid": "1.0.0",
    "luster-log-winston": "1.1.0",
    "node-statsd": "^0.1.1",
    "qs": "^6.9.4",
    "ua-parser-js": "^0.7.10",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "grunt": "^0.4.5",
    "husky": "3.1.0",
    "jest": "25.1.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1"
  }
}
