{
    "meta": {
        "name": "mj-test-service",
        "maintainer": "daniilpozdeev <daniilpozdeev@yandex-team.ru>"
    },
    "include": [
        "market/infra/java-application/core/package-include.json"
    ],
    "data": [
        {
            "source": {
                "type": "RELATIVE",
                "path": "service.yaml"
            },
            "destination": {
                "path": "/{package_name}/service.yaml"
            }
        },
        {
            "source": {
                "type": "RELATIVE",
                "path": "supervisor.conf"
            },
            "destination": {
                "path": "/{package_name}/supervisor.conf"
            }
        }
    ]
}
