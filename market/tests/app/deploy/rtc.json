{
    "meta": {
        "name": "app",
        "maintainer": "yuraaka",
        "description": "app",
        "version": "{revision}"
    },
    "build": {
        "targets": [
            "market/library/shiny/server/gen/lib/tests/app/bin"
        ]
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "market/library/shiny/server/gen/lib/tests/app/bin/bin"
            },
            "destination": {
                "path": "/root/bin/app"
            }
        }, {
            "source": {
                "type": "ARCADIA",
                "path": "market/library/shiny/server/gen/lib/tests/app/bin/cfg"
            },
            "destination": {
                "path": "/root/conf/app"
            }
        }, {
           "source": {
               "type": "ARCADIA",
               "path": "market/library/shiny/server/gen/lib/tests/app/deploy/logrotate.conf"
           },
           "destination": {
               "path": "/root/tmpl/conf/logrotate.d/app"
           }
        }
    ]
}
