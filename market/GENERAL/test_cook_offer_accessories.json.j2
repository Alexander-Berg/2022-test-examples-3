{% if table == "sets_offer_accessories" %}
    {% table table %}
        ["O1", 2000000000, "O2,O1,O3", "{{ day }}"],
        ["O2", 2000000000, "O1,O4", "{{ day }}"]
    {% endtable %}
{% elif table == "sets_offer_accessories_scores" %}
    {% table table %}
        ["O1", "O1", 0.5, "{{ day }}"],
        ["O1", "O2", 1.0, "{{ day }}"],
        ["O1", "O3", 0.1, "{{ day }}"],
        ["O2", "O1", 6.6, "{{ day }}"],
        ["O2", "O4", 2.2, "{{ day }}"]
    {% endtable %}
{% endif %}
