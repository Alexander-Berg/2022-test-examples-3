{% set table = table|default("statistics_multiplication") %}

{% if table == "statistics_multiplicand" %}
    {% table "statistics" %}
        ["Desktop", "Total", "total", 50.0, "{{ day }}"],
        ["Touch", "Total", "total", 50.0, "{{ day }}"],
        ["Android", "Total", "total", 50.0, "{{ day }}"],
        ["Market", "Total", "total", 150.0, "{{ day }}"]
    {% endtable %}
{% elif table == "statistics_multiplier" %}
    {% table "statistics" %}
        ["Desktop", "Total", "total", 0.1, "{{ day }}"],
        ["Touch", "Total", "total", 0.1, "{{ day }}"],
        ["IOSMainPage", "Total", "total", 0.1, "{{ day }}"],
        ["Market", "Total", "total", 0.3, "{{ day }}"]
    {% endtable %}
{% elif table == "statistics_multiplication" %}
    {% table "statistics" %}
        ["Desktop", "Total", "total", 5.0, "{{ day }}"],
        ["Touch", "Total", "total", 5.0, "{{ day }}"],
        ["Market", "Total", "total", 45.0, "{{ day }}"]
    {% endtable %}
{% endif %}
