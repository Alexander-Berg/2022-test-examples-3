{% if table == "sets_reasons_to_buy_best_by_factor" %}
    {# result data #}
    {% table "sets_reasons_to_buy" %}
        {% for i in range(9) %}
            ["C1Z{{ i }}", "model", "best_by_factor", "consumerFactor", {{ 10 / 110.0 }}, {"factor_id": "F2", "factor_name": "f2", "factor_priority": "1", "value_threshold": "0.8"}, "{{ day }}"],
            ["C1Z{{ i }}", "model", "best_by_factor", "consumerFactor", {{ 10 / 120.0 }}, {"factor_id": "F1", "factor_name": "f1", "factor_priority": "2", "value_threshold": "0.8"}, "{{ day }}"],
        {% endfor %}
        {% for i in range(9) %}
            ["C2Z{{ i }}", "model", "best_by_factor", "consumerFactor", 0.0, {"factor_id": "F3", "factor_name": "f3", "factor_priority": "1", "value_threshold": "0.8"}, "{{ day }}"],
        {% endfor %}
        ["M1", "model", "best_by_factor", "consumerFactor", {{ 10 / 12.0 }}, {"factor_id": "F1", "factor_name": "f1", "factor_priority": "2", "value_threshold": "0.8"}, "{{ day }}"],
        ["M1", "model", "best_by_factor", "consumerFactor", {{ 10 / 11.0 }}, {"factor_id": "F2", "factor_name": "f2", "factor_priority": "1", "value_threshold": "0.8"}, "{{ day }}"],
        ["M2", "model", "best_by_factor", "consumerFactor", {{ 10 / 12.0 }}, {"factor_id": "F1", "factor_name": "f1", "factor_priority": "1", "value_threshold": "0.8"}, "{{ day }}"],
        ["M2", "model", "best_by_factor", "consumerFactor", 0.0, {"factor_id": "F2", "factor_name": "f2", "factor_priority": "2", "value_threshold": "0.8"}, "{{ day }}"],
        {% if not version.startswith("blue") %}
            ["M2M", "model", "best_by_factor", "consumerFactor", {{ 10 / 12.0 }}, {"factor_id": "F1", "factor_name": "f1", "factor_priority": "1", "value_threshold": "0.8"}, "{{ day }}"],
            ["M2M", "model", "best_by_factor", "consumerFactor", 0, {"factor_id": "F2", "factor_name": "f2", "factor_priority": "2", "value_threshold": "0.8"}, "{{ day }}"],
        {% endif %}
        ["M3", "model", "best_by_factor", "consumerFactor", {{ 9 / 10.0 }}, {"factor_id": "F3", "factor_name": "f3", "factor_priority": "1", "value_threshold": "0.8"}, "{{ day }}"],
        ["M4", "model", "best_by_factor", "consumerFactor", 0.0, {"factor_id": "F1", "factor_name": "f1", "factor_priority": "1", "value_threshold": "0"}, "{{ day }}"]
    {% endtable %}
{% elif table == "items_category_tree" %}
    {% table table %}
        ["C1", "C", ["C"], "category", "{{ day }}"],
        ["C2", "C", ["C"], "category", "{{ day }}"],
        ["1003092", "C", ["C"], "category", "{{ day }}"],
        ["C3", "1003092", ["C", "1003092"], "category", "{{ day }}"],
        ["CATEGORY_FOR_TEST", "C", ["C"], "category", "{{ day }}"]
    {% endtable %}
{% elif table == "items_factor_name" %}
    {% table table %}
        ["F1", "f1", "factor", "{{ day }}"],
        ["F2", "f2", "factor", "{{ day }}"],
        ["F3", "f3", "factor", "{{ day }}"],
        ["110", "banned", "factor", "{{ day }}"]
    {% endtable %}
{% elif table == "items_model_category_id" %}
    {% table table %}
        {% for i in range(9) %}
            ["C1Z{{ i }}", "C1", "model", "{{ day }}"],
        {% endfor %}
        ["M1", "C1", "model", "{{ day }}"],
        ["M2", "C1", "model", "{{ day }}"],
        {% for i in range(9) %}
            ["C2Z{{ i }}", "C2", "model", "{{ day }}"],
        {% endfor %}
        ["M3", "C2", "model", "{{ day }}"],
        ["M4", "CATEGORY_FOR_TEST", "model", "{{ day }}"],
        {# these models should not pass because of banned category #}
        {% for i in range(9) %}
            ["C3Z{{ i }}", "C3", "model", "{{ day }}"],
        {% endfor %}
        ["M5", "C3", "model", "{{ day }}"]
    {% endtable %}
{% elif table == "items_model_factors" %}
    {% table table %}
        {% for i in range(9) %}
            ["C1Z{{ i }}", {"F1": 4.0, "F2": 4.0}, "model", "{{ day }}"],
        {% endfor %}
        ["M1", {"F1": 5.0, "F2": 4.5}, "model", "{{ day }}"],
        ["M2", {"F1": 5.0, "F2": 3.0}, "model", "{{ day }}"],
        {% for i in range(9) %}
            ["C2Z{{ i }}", {"F3": 4.8, "110": 4.0}, "model", "{{ day }}"],
        {% endfor %}
        ["M3", {"F3": 4.8, "110": 5.0}, "model", "{{ day }}"],
        {% for i in range(9) %}
            ["C3Z{{ i }}", {"F1": 3.0}, "model", "{{ day }}"],
        {% endfor %}
        ["M4", {"F1": 3.0}, "model", "{{ day }}"],
        ["M5", {"F1": 5.0}, "model", "{{ day }}"]
    {% endtable %}
{% elif table == "items_model_feedback_statistics" %}
    {% table table %}
        {% for i in range(9) %}
            ["C1Z{{ i }}", 4.5, 53, 15, 0, 10, 4.5, "model", "{{ day }}"],
        {% endfor %}
        ["M1", 4.5, 53, 15, 0, 10, 4.5, "model", "{{ day }}"],
        ["M2", 4.5, 53, 15, 0, 10, 4.5, "model", "{{ day }}"],
        {% for i in range(9) %}
            ["C2Z{{ i }}", 4.5, 53, 15, 0, 10, 4.5, "model", "{{ day }}"],
        {% endfor %}
        ["M3", 4.5, 53, 15, 0, 100, 4.5, "model", "{{ day }}"],
        {% for i in range(9) %}
            ["C3Z{{ i }}", 4.5, 53, 15, 0, 10, 4.5, "model", "{{ day }}"],
        {% endfor %}
        ["M4", 4.5, 53, 15, 0, 10, 4.5, "model", "{{ day }}"],
        ["M5", 4.5, 53, 15, 0, 10, 4.5, "model", "{{ day }}"]
    {% endtable %}
{% elif table == "items_model_group" %}
    {% table table %}
        ["M2M", "M2", "model", "{{ day }}"],
        {# this relation must be ignored, since there is some statistics for M1 #}
        ["M1", "M2", "model", "{{ day }}"]
    {% endtable %}
{% elif table == "items_sku_model_id" %}
