<?xml version="1.0" encoding="UTF-8"?>
<root>
    <token>%9$s</token>
    <hash>%1$s</hash>
    <uniq>%1$s</uniq>
    <request type="createTransfer">
        <transfer>
            <transferId>
                <yandexId>%2$s</yandexId>
            </transferId>
            <from>%3$s</from>
            <to>%4$s</to>
            <transferItems>
                <transferItem>
                    <unitId>
                        <sku>%6$s</sku>
                        <vendorId>%7$s</vendorId>
                        <article>%8$s</article>
                    </unitId>
                    <count>%5$s</count>
                    <instances>
                        <instance>
                            <partialIds>%10$s</partialIds>
                        </instance>
                    </instances>
                </transferItem>
            </transferItems>
        </transfer>
    </request>
</root>
