                                    "pattern": "^(.*)$"
                                },
                                "id": {
                                    "$id": "#/properties/offers/items/properties/discounts/items/properties/id",
                                    "type": "string",
                                    "title": "The Id Schema",
                                    "default": "",
                                    "examples": [
                                        ""
                                    ],
                                    "pattern": "^(.*)$"
                                }
                            }
                        }
                    }
                }
            }
        },
