                                "$id": "#/properties/offers/items/properties/car_info/properties/sf",
                                "type": "array",
                                "title": "The Sf Schema",
                                "items": {
                                    "$id": "#/properties/offers/items/properties/car_info/properties/sf/items",
                                    "type": "integer",
                                    "title": "The Items Schema",
                                    "default": 0,
                                    "examples": [
                                        158
                                    ]
                                }
                            },
                            "model_id": {
                                "$id": "#/properties/offers/items/properties/car_info/properties/model_id",
                                "type": "string",
                                "title": "The Model_id Schema",
                                "default": "",
                                "examples": [
                                    "kia_rio_xline"
                                ],
                                "pattern": "^(.*)$"
                            },
                            "telematics": {
                                "$id": "#/properties/offers/items/properties/car_info/properties/telematics",
                                "type": "object",
                                "title": "The Telematics Schema",
                                "properties": {
                                    "fuel_distance": {
                                        "$id": "#/properties/offers/items/properties/car_info/properties/telematics/properties/fuel_distance",
                                        "anyOf": [
                                            {
                                                "type": "integer"
                                            },
                                            {
                                                "type": "null"
                                            }
                                        ],
                                        "title": "The Fuel_distance Schema"
                                    },
                                    "fuel_level": {
                                        "$id": "#/properties/offers/items/properties/car_info/properties/telematics/properties/fuel_level",
                                        "anyOf": [
                                            {
                                                "type": "integer"
                                            },
                                            {
                                                "type": "null"
                                            }
                                        ],
                                        "title": "The Fuel_level Schema"
                                    }
                                }
                            }
                        }
                    },
                    "discounts": {
                        "$id": "#/properties/offers/items/properties/discounts",
                        "type": [
                            "array",
                            "null"
                        ],
                        "title": "The Discounts Schema",
                        "items": {
                            "$id": "#/properties/offers/items/properties/discounts/items",
                            "type": "object",
                            "title": "The Items Schema",
