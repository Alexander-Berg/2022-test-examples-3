                    "index": {
                        "$id": "#/properties/sf/items/properties/index",
                        "type": "integer",
                        "title": "The Index Schema",
                        "default": 0,
                        "examples": [
                            158
                        ]
                    },
                    "action_name": {
                        "$id": "#/properties/sf/items/properties/action_name",
                        "type": "string",
                        "title": "The Action_name Schema",
