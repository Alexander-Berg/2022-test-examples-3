                        "pattern": "^(.*)$"
                    },
                    "group_name": {
                        "$id": "#/properties/offers/items/properties/group_name",
                        "type": "string",
                        "title": "The Group_name Schema",
                        "default": "",
                        "examples": [
                            ""
