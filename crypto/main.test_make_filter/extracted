([/meta/type_code] = 3 OR [/meta/type_code] = 4 OR [/meta/type_code] = 5 OR [/meta/type_code] = 10) AND ([/spec/update_period_hours] > 0u OR [/spec/processing_info/last_scheduled_time] = 0u) AND (uint64([/spec/processing_info/last_scheduled_time]) + uint64([/spec/update_period_hours]) * 3600 <= 150000000u)