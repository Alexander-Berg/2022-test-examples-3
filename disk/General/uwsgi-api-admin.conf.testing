[uwsgi]
socket = [::]:9000
processes = 2
enable-threads = true
master = true
thunder-lock = true
buffer-size = 32768
wsgi-file = /usr/bin/wsgi_admin.py
pythonpath = /etc/yandex/mpfs-api-admin/conf/app
pythonpath = /etc/yandex/mpfs-api-admin/conf/app/overrides
env = DJANGO_SETTINGS_MODULE=testing_settings
