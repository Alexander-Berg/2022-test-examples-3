                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        A policy of granting current offer
                    </xsd:documentation>
                </xsd:annotation>

                <xsd:sequence>
                    <xsd:element name="policy" type="tns:OfferPolicy" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Правило предоставления предложения.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                A policy of granting current offer.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="OfferPolicy">
                <xsd:sequence>
                    <xsd:element name="type" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Тип правила предоставления предложения.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                A type of policy of granting current offer.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element name="value" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Значение правила предоставления предложения.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                A value of policy of granting current offer.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="ChangeList">
                <xsd:sequence>
                    <xsd:element type="tns:AbstractChange" name="change" maxOccurs="unbounded"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="Change">
