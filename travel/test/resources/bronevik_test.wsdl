                            <xsd:documentation xml:lang="en">
                                Details of the minimum price at the hotel.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="geo" type="tns:HotelGeo" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Геоинформация об отеле.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Geo information about the hotel.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="info" type="tns:HotelInfo" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Информация об отеле.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Information about the hotel.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="amenities" type="tns:AvailableAmenities" minOccurs="0">
