            <xsd:complexType name="HotelWithCheapestOffer">
                <xsd:sequence>
                    <xsd:element name="id" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Идентификатор отеля.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Hotel ID.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
