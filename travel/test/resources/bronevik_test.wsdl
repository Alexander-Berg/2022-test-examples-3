                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Coordinates of the hotel.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="Coordinates">
                <xsd:sequence>
                    <xsd:element name="latitude" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Широта.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Latitude.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="longitude" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Долгота.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Longitude.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="HotelInfo">
                <xsd:sequence>
                    <xsd:element name="taxes" type="tns:Taxes">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Дополнительные сборы при заселении
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The additional fees at check-in
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="description" type="xsd:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Описание отеля.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Description of the hotel.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="name" type="xsd:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Название отеля.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Hotel name.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="category" type="xsd:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Категория отеля.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Hotel category.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="type" type="xsd:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Тип отеля.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The type of the hotel.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="photo" type="tns:Image" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Главная фотография отеля.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Main photo of the hotel.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="deepLink" type="xsd:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Элемент для работы метапоисковых систем.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The element for metasearch engines.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="paymentTerms" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Условия оплаты.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Payment terms.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="HotelVatInfo">
                <xsd:sequence>
                    <xsd:element name="applicable" type="xsd:boolean">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Применим ли к отелю НДС.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Returns `true` for properties located in Russian Federation and `false` for foreign
                                properties.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="included" type="xsd:boolean">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Включен ли НДС в стоимость.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Flag of whether VAT is included in the client’s price.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="percent" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Процент НДС в отеле.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Percentage of VAT at the hotel.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="BaseOffer">
                <xsd:sequence>
                    <xsd:element name="name" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Название предложения
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The offer name
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="roomId" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Идентификатор номера
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The room id
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="cancellationPolicies" type="tns:HotelOfferCancellationPolicy"
                                 maxOccurs="unbounded" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Список политик аннуляций
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The list of cancellation policies
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="meals" type="tns:AvailableMeals" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Список доступных типов питания
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The list of available meal services
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="freeRooms" type="xsd:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Количество доступных номеров
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The amount of available rooms within this offer
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="paymentRecipient" type="tns:PaymentRecipients">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Место оплаты
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Specifies the payment scheme
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="roomType" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Тип размещения в номере
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The room type
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="isSharedRoom" type="xsd:boolean">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Является ли номер номером с подселением
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Specifies whether the room is dorm or private
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="isBlockRoom" type="xsd:boolean">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Является ли номер блочным (Блочный номер - это номер с общей ванной комнатой и туалетом
                                для нескольких номеров)
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Specifies whether the bathroom is located in the hallway and shared by all of the guests
                                on that floor
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="nonRefundable" type="xsd:boolean">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Флаг невозвратности предложения
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Whether the offer is non-refundable
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="rateType" type="tns:RateType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Информация о типе тарифа.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Information about rate type.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="guaranteeType" type="xsd:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Информация о типе гарантии.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Information about guarantee type.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="availableBedSets" type="tns:BedSets" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Варианты комбинаций кроватей.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Available sets of beds in the room.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="childrenAccommodation" type="tns:ChildrenAccommodation" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Места для детей.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Children Accommodation.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="SearchAvailabilityHotelOffer">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseOffer">
                        <xsd:sequence>
                            <xsd:element name="photo" type="tns:Image" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Фотография отеля.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Hotel photo.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="amenities" type="tns:AvailableAmenities" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Доступные в отеле доп. услуги.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Available amenities in the property.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="RateType">
                <xsd:sequence>
                    <xsd:element name="rateName" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Тип тарифа.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Rate type.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="rateDescription" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Описание условий предоставления тарифа.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The description of rate conditions.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:element name="UpdateServiceRequest"
                         type="tns:UpdateServiceRequest">
            </xsd:element>

            <xsd:element name="UpdateServiceResponse"
                         type="tns:UpdateServiceResponse">
            </xsd:element>

            <xsd:complexType name="UpdateServiceRequest">
				<xsd:annotation>
					<xsd:documentation xml:lang="ru">Изменить услугу</xsd:documentation>
					<xsd:documentation xml:lang="en">Change the booking</xsd:documentation>
				</xsd:annotation>
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseRequest">
                        <xsd:sequence>
                            <xsd:element name="serviceId" type="xsd:int">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Идентификатор изменяемой услуги.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The identifier of the service to be changed.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="updateService" type="tns:UpdateService"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="UpdateServiceResponse">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseResponse">
                        <xsd:sequence>
                            <xsd:element name="order" type="tns:Order" />
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>


            <xsd:element name="CancelServicesRequest"
                         type="tns:CancelServicesRequest">
            </xsd:element>

            <xsd:element name="CancelServicesResponse"
                         type="tns:CancelServicesResponse">
            </xsd:element>

            <xsd:complexType name="CancelServicesRequest">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseRequest">
                        <xsd:sequence>
                            <xsd:element name="serviceIds" type="tns:ServiceIdsForCancellation">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Идентификаторы услуг, которые необходимо аннулировать.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Identifiers of the services to be canceled.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="CancelServicesResponse">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseResponse">
                        <xsd:sequence>
                            <xsd:element name="services" type="tns:CancelledServices">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Результат аннуляции услуг.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Result of cancellation of services.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="ServiceIdsForCancellation">
                <xsd:sequence>
                    <xsd:element name="id" type="xsd:int" maxOccurs="6">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Массив идентификаторов услуг, которые необходимо аннулировать.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Array of identifiers of the services to be canceled.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="CancelledServices">
                <xsd:sequence>
                    <xsd:element name="service" type="tns:CancelledService" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Массив результата аннуляции услуг.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Array of services cancellation result.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="CancelledService">
                <xsd:sequence>
                    <xsd:element name="id" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Идентификатор услуги.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Service ID.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element name="status" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Статус услуги.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Service Status.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element name="result" type="xsd:boolean">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Результат аннуляции услуги (true или false).
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The result of the cancellation of the service (true or false).
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:element name="GetCheckinCheckoutPricingRequest"
                         type="tns:GetCheckinCheckoutPricingRequest">
            </xsd:element>

            <xsd:element name="GetCheckinCheckoutPricingResponse"
                         type="tns:GetCheckinCheckoutPricingResponse">
            </xsd:element>

            <xsd:complexType name="GetCheckinCheckoutPricingRequest">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseRequest">
                        <xsd:sequence>
                            <xsd:element name="currency" type="tns:Currency">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Валюта расчета
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The payment currency
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="offerCodes" type="tns:OfferCodes">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Коды предложений, по которым будет осуществлен поиск.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The offers codes that will be searched.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="OfferCodes">
                <xsd:sequence>
                    <xsd:element name="offerCode" type="xsd:string" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Массив кодов предложений.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Array of offer codes.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="GetCheckinCheckoutPricingResponse">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseResponse">
                        <xsd:sequence>
                            <xsd:element name="checkinCheckoutPrices" type="tns:OffersCheckinCheckoutPrices">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Офферы с ценами за ранний заезд и поздний выезд.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Offers with prices for early arrival and late departure.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="OffersCheckinCheckoutPrices">
                <xsd:sequence>
                    <xsd:element name="offerPrices" type="tns:OfferCheckinCheckoutPrices" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Массив офферов с ценами РЗ и ПВ.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Array of offers with prices EA and LD.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="ClaimCheckinCheckoutPrices">
                <xsd:sequence>
                    <xsd:element name="checkin" type="tns:OfferHourPrices" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Цены за ранний заезд.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Prices for early arrival.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="checkout" type="tns:OfferHourPrices" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Цены за поздний выезд.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Prices for late departure.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="OfferCheckinCheckoutPrices">
                <xsd:sequence>
                    <xsd:element name="offerCode" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Код предложения.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Offer code.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="checkin" type="tns:OfferHourPrices">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Цены за ранний заезд.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Prices for early arrival.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="checkout" type="tns:OfferHourPrices">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Цены за поздний выезд.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Prices for late departure.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="OfferHourPrices">
                <xsd:sequence>
                    <xsd:element name="hourPrice" type="tns:OfferHourPrice" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Массив с ценами по часам.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Array with prices by the hour.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="OfferHourPrice">
                <xsd:sequence>
                    <xsd:element name="hour" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Час.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Hour.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="availabilityCode" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Код доступности.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Availability Code.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="priceDetails" type="tns:PriceDetails" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Детализация стоимости часа заезда/выезда.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Detailing of check-in/check-out amount for the hour.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="EditServiceHourPrice">
                <xsd:complexContent>
                    <xsd:extension base="tns:OfferHourPrice">
