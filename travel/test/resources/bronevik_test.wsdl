                            <xsd:documentation xml:lang="ru">Контактный e-mail для подтверждения бронирования
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">The contact email, we use this address to send
                                confirmations
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="comment" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Комментарии к заказу</xsd:documentation>
                            <xsd:documentation xml:lang="en">The comment to the order</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="services" type="tns:OrderServices">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Список возможных услуг</xsd:documentation>
                            <xsd:documentation xml:lang="en">List of the services</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="OrderServices">
                <xsd:sequence>
                    <xsd:element name="service" type="tns:OrderService" maxOccurs="unbounded"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="OrderService" abstract="true">
                <xsd:sequence>
                    <xsd:element name="id" type="xsd:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Идентификатор услуги</xsd:documentation>
                            <xsd:documentation xml:lang="en">The service id</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="date" type="xsd:dateTime">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Дата создания услуги</xsd:documentation>
                            <xsd:documentation xml:lang="en">The creating order date</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="referenceId" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Номер услуги в системе клиента</xsd:documentation>
                            <xsd:documentation xml:lang="en">The service number in the client’s system
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="cityId" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Идентификатор города, где находится отель
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">The id of the city where the hotel is located
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="cityName" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Название города</xsd:documentation>
                            <xsd:documentation xml:lang="en">The name of the city where the hotel is located
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="countryId" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Идентификатор страны, где находится отель
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">The id of the country where the hotel is located
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="countryName" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Название страны</xsd:documentation>
                            <xsd:documentation xml:lang="en">The name of the country where the hotel is located
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="priceDetails" type="tns:PriceDetails">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Детализация стоимости заказа</xsd:documentation>
                            <xsd:documentation xml:lang="en">The detailed order price</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="cancellationPolicies"
                                 type="tns:CancellationPolicy" maxOccurs="unbounded" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Правила отмены брони</xsd:documentation>
                            <xsd:documentation xml:lang="en">The policies of booking cancellation</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="statusId" type="xsd:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Статус услуги
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The service status
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="statusName" type="xsd:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Название статуса услуги
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The service name
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:element name="GetOrderRequest" type="tns:GetOrderRequest"/>
            <xsd:element name="GetOrderResponse" type="tns:GetOrderResponse"/>
            <xsd:complexType name="GetOrderRequest">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseRequest">
                        <xsd:sequence>
                            <xsd:element name="orderId" type="xsd:int"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>
            <xsd:complexType name="GetOrderResponse">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseResponse">
                        <xsd:sequence>
                            <xsd:element name="order" type="tns:Order"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>
            <xsd:complexType name="Orders">
                <xsd:sequence>
                    <xsd:element name="order" type="tns:Order" maxOccurs="unbounded"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="DailyPrices">
                <xsd:sequence>
                    <xsd:element name="dailyPrice" type="tns:DailyPrice" minOccurs="0" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Детализация стоимости
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The detailed daily price
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="OrderServiceAccommodation">
                <xsd:complexContent>
                    <xsd:extension base="tns:OrderService">
                        <xsd:sequence>
                            <xsd:element name="contract" type="tns:Contract">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Договор, на который оформлено бронирование
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The information about contract of booking
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="offerCode" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Код предложения
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The offer code
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="offerName" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Название предложения
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The offer name
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="hotelId" type="xsd:int">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Идентификатор отеля
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The hotel id
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="hotelName" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Название отеля
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The hotel name
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="roomId" type="xsd:int">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Идентификатор номера
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The room id
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="roomType" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Тип размещения в номере
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The room type
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="checkin" type="xsd:dateTime">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Дата и время заезда
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The check-in date and time
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="checkout" type="xsd:dateTime">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Дата и время выезда
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The check-out date and time
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="guests" type="tns:Guests">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        ФИО гостей
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The name of guests
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="children" type="tns:ChildrenAccommodation" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Места для детей.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Children Accommodation.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="comment" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Комментарии
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        An important information about accommodation
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="nonRefundable" type="xsd:boolean">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Флаг невозвратности предложения
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Whether the offer is non-refundable
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:sequence>
                                <xsd:element name="meals"
                                             type="tns:AvailableMeals" minOccurs="0">
                                    <xsd:annotation>
                                        <xsd:documentation xml:lang="ru">
                                            Информация о питании
                                        </xsd:documentation>
                                        <xsd:documentation xml:lang="en">
                                            An information about meal service
                                        </xsd:documentation>
                                    </xsd:annotation>
                                </xsd:element>
                            </xsd:sequence>
                            <xsd:element name="isBlockRoom" type="xsd:boolean">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Является ли номер блочным. Блочный номер - это номер с общей ванной комнатой и
                                        туалетом для нескольких номеров
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Specifies whether the bathroom is located in the hallway and shared by all of
                                        the guests on that floor
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="isSharedRoom" type="xsd:boolean">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Является ли номер номером с подселением
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Specifies whether the room is dorm or private
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="paymentRecipient" type="tns:PaymentRecipients">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Место оплаты
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Specifies the payment scheme
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="extraField" type="tns:ServiceExtraField" maxOccurs="unbounded"
                                         minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Доп. поля услуг
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Extra fields for services
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:sequence>
                                <xsd:element name="offerPolicies" type="tns:OfferPolicies">
                                    <xsd:annotation>
                                        <xsd:documentation xml:lang="ru">
                                            Правила предоставления заказа
                                        </xsd:documentation>
                                        <xsd:documentation xml:lang="en">
                                            A policies of granting current offer
                                        </xsd:documentation>
                                    </xsd:annotation>
                                </xsd:element>
                            </xsd:sequence>
                            <xsd:sequence>
                                <xsd:element name="dailyPrices" type="tns:DailyPrices" minOccurs="0">
                                    <xsd:annotation>
                                        <xsd:documentation xml:lang="ru">
                                            Детализация стоимости на каждый день проживания
                                        </xsd:documentation>
                                        <xsd:documentation xml:lang="en">
                                            The detailed daily cost of accommodation
                                        </xsd:documentation>
                                    </xsd:annotation>
                                </xsd:element>
                            </xsd:sequence>
                            <xsd:element name="VATPercent" type="xsd:int">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">Размер НДС в процентах.</xsd:documentation>
                                    <xsd:documentation xml:lang="en">VAT percentage rate</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="rateType" type="tns:RateType">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Информация о типе тарифа.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Information about rate type.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="guaranteeType" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Информация о типе гарантии.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Information about guarantee type.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="paymentTerms" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Условия оплаты.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Payment terms.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="preferredBedSet" type="tns:BedSet" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Предпочитаемые кровати
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Preferred Beds
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>
            <xsd:element name="UpdateOrderRequest" type="tns:UpdateOrderRequest"/>
            <xsd:element name="UpdateOrderResponse" type="tns:UpdateOrderResponse"/>
            <xsd:element name="CancelOrderRequest" type="tns:CancelOrderRequest"/>
            <xsd:element name="CancelOrderResponse" type="tns:CancelOrderResponse"/>
            <xsd:complexType name="CancelOrderRequest">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseRequest">
                        <xsd:sequence>
                            <xsd:element name="orderId" type="xsd:int"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>
            <xsd:complexType name="CancelOrderResponse">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseResponse">
                        <xsd:sequence>
                            <xsd:element name="result"
                                         type="xsd:boolean">
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>
            <xsd:element name="SearchOrdersRequest" type="tns:SearchOrdersRequest"/>
            <xsd:element name="SearchOrdersResponse" type="tns:SearchOrdersResponse"/>
            <xsd:element name="GetHotelInfoRequest" type="tns:GetHotelInfoRequest"/>
            <xsd:element name="GetHotelInfoResponse" type="tns:GetHotelInfoResponse"/>
            <xsd:complexType name="GetHotelInfoRequest">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseRequest">
                        <xsd:sequence>
                            <xsd:element name="hotelIds" type="tns:HotelIds" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Идентификаторы отелей, по которым будет осуществлен поиск.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The identifiers of the hotels that will be searched.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="cityIds" type="tns:CityIds" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Возможность получить информацию по отелям из указанных городов.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Possibility to get information on hotels from the specified cities.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>
            <xsd:complexType name="GetHotelInfoResponse">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseResponse">
                        <xsd:sequence>
                            <xsd:element name="hotels" type="tns:HotelsWithInfo" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>
            <xsd:complexType name="Image">
                <xsd:sequence>
                    <xsd:element name="guid" type="xsd:string"/>
                    <xsd:element name="url" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="Tax">
                <xsd:sequence>
                    <xsd:element name="type" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Тип сбора
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The type of fee
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="amount" type="xsd:float">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Величина сбора
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The amount of fee
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="currency" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Валюта оплаты
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The payment fee
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="included" type="xsd:boolean">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Включен ли сбор в стоимость предложения
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Is the fee included in the price of offer
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="comment" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Комментарии
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                An information about taxes
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="HotelRoom">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Описание номера отеля</xsd:documentation>
                    <xsd:documentation xml:lang="en">The hotel room description</xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                    <xsd:element name="id" type="xsd:int"/>
                    <xsd:element name="name" type="xsd:string"/>

                    <xsd:element name="availableAmenities"
                                 type="tns:AvailableAmenities" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Доступные в номере удобства
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Available amenities in room
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="description" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Описание номера</xsd:documentation>
                            <xsd:documentation xml:lang="en">The text room description</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="photos" type="tns:Photos">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Фотографии номера</xsd:documentation>
                            <xsd:documentation xml:lang="en">Room's photos</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="roomCapacity" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Количество гостей, которых можно разместить в номере
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                An amount of guests that can be accommodated in the room
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="size" type="xsd:float" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Площадь номера
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Room size
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="availableBedSets" type="tns:BedSets">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Варианты комбинаций кроватей.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Available sets of beds in the room.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="buildingPart" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Часть здания, в которой находится номер.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The part of the building where the room is located.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="bedroomAmount" type="xsd:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Количество комнат в номере.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The number of apartment in the room.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="withWindow" type="xsd:boolean">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Информация о наличии окна в номере.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Information about the availability of windows in the room.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="windowViews" type="tns:WindowViews" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Информация о видах из окна в номере.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Information about views from the window in the room.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="WindowViews">
                <xsd:sequence>
                    <xsd:element name="name" type="xsd:string" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Название вида из окна.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The name of the view from the window.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="BedSets">
                <xsd:sequence>
                    <xsd:element name="bedSet" type="tns:BedSet" maxOccurs="unbounded" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Массив вариантов комбинаций кроватей.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Element describes the available sets of beds in the room.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="BedSet">
                <xsd:sequence>
                    <xsd:element name="bed" type="tns:Bed" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Вариант размещения кроватей.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The option of beds combinations.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="Bed">
                <xsd:sequence>
                    <xsd:element name="type" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Тип размещения в номере
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The room type
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="amount" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Количество кроватей
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Beds count
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="SearchOfferCriterion" abstract="true"/>
            <xsd:complexType name="SearchOrderCriterion" abstract="true"/>

            <xsd:complexType name="SearchOrderCriterionOrderId">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Поиск заказа по ID</xsd:documentation>
                    <xsd:documentation xml:lang="en">The order search by order ID</xsd:documentation>
                </xsd:annotation>
                <xsd:complexContent>
                    <xsd:extension base="tns:SearchOrderCriterion">
                        <xsd:sequence>
                            <xsd:element name="orderId" type="xsd:int"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="SearchOrderCriterionServiceReferenceId">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Поиск заказа по номеру услуги в системе клиента</xsd:documentation>
                    <xsd:documentation xml:lang="en">The order search by client’s service number</xsd:documentation>
                </xsd:annotation>
                <xsd:complexContent>
                    <xsd:extension base="tns:SearchOrderCriterion">
                        <xsd:sequence>
                            <xsd:element name="referenceId" type="xsd:string"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="SearchOrderCriterionServiceId">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Поиск заказа по номеру услуги проживания</xsd:documentation>
                    <xsd:documentation xml:lang="en">The order search by service ID</xsd:documentation>
                </xsd:annotation>
                <xsd:complexContent>
                    <xsd:extension base="tns:SearchOrderCriterion">
                        <xsd:sequence>
                            <xsd:element name="serviceId" type="xsd:int"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="SearchOrdersRequest">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseRequest">
                        <xsd:sequence>
                            <xsd:element name="searchCriteria" type="tns:SearchOrderCriterion" maxOccurs="unbounded"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>
            <xsd:complexType name="SearchOrdersResponse">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseResponse">
                        <xsd:sequence>
                            <xsd:element name="orders" type="tns:Orders"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="UpdateOrderRequest">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseRequest">
                        <xsd:sequence>
                            <xsd:element name="order" type="tns:Order"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>
            <xsd:complexType name="UpdateOrderResponse">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseResponse"/>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="SearchOrderCriterionCreateDate">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Поиск по дате создания</xsd:documentation>
                    <xsd:documentation xml:lang="en">The order search by order creation date</xsd:documentation>
                </xsd:annotation>
                <xsd:complexContent>
                    <xsd:extension base="tns:SearchOrderCriterion">
                        <xsd:sequence>
                            <xsd:element name="dateStart" type="xsd:date"/>
                            <xsd:element name="dateEnd" type="xsd:date"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>
            <xsd:complexType name="SearchOrderCriterionArrivalDate">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Поиск по дате заезда</xsd:documentation>
                    <xsd:documentation xml:lang="en">The order search by check-in date</xsd:documentation>
                </xsd:annotation>
                <xsd:complexContent>
                    <xsd:extension base="tns:SearchOrderCriterion">
                        <xsd:sequence>
                            <xsd:element name="dateStart" type="xsd:date"/>
                            <xsd:element name="dateEnd" type="xsd:date"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>
            <xsd:complexType name="SearchOrderCriterionDepartureDate">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Поиск заказа по дате выезда</xsd:documentation>
                    <xsd:documentation xml:lang="en">The order search by end date</xsd:documentation>
                </xsd:annotation>
                <xsd:complexContent>
                    <xsd:extension base="tns:SearchOrderCriterion">
                        <xsd:sequence>
                            <xsd:element name="dateStart" type="xsd:date"/>
                            <xsd:element name="dateEnd" type="xsd:date"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>
            <xsd:complexType name="SearchOrderCriterionGuest">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Поиск по именам гостей
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        The order search by guest name
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:complexContent>
                    <xsd:extension base="tns:SearchOrderCriterion">
                        <xsd:sequence>
                            <xsd:element name="name" type="xsd:string"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="HotelOfferCancellationPolicy">
                <xsd:complexContent>
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Правила аннуляции предложения проживания</xsd:documentation>
                        <xsd:documentation xml:lang="en">The hotel offer cancellation policy</xsd:documentation>
                    </xsd:annotation>
                    <xsd:extension base="tns:CancellationPolicy">
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="CancellationPolicy" abstract="true">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Правила аннуляции</xsd:documentation>
                    <xsd:documentation xml:lang="en">A cancellation policy</xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                    <xsd:element name="penaltyDateTime" type="xsd:dateTime">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Время наступления штрафных санкций.</xsd:documentation>
                            <xsd:documentation xml:lang="en">Cancellation date and time (hotel time)</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="penaltyPriceDetails" type="tns:ClientPriceDetails" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Детализация штрафа.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Detailing of penalty.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="SearchOfferCriterionNumberOfGuests">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Поиск по количеству человек</xsd:documentation>
                    <xsd:documentation xml:lang="en">The offer search by number of guests</xsd:documentation>
                </xsd:annotation>
                <xsd:complexContent>
                    <xsd:extension base="tns:SearchOfferCriterion">
                        <xsd:sequence>
                            <xsd:element name="adults">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">Количество взрослых</xsd:documentation>
                                    <xsd:documentation xml:lang="en">Number of adult guests</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:int">
                                        <xsd:minInclusive value="1"/>
                                        <xsd:maxInclusive value="2"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="children" type="tns:Child" maxOccurs="unbounded" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="SearchOfferCriterionGuaranteeTypes">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Поиск по типам гарантии</xsd:documentation>
                    <xsd:documentation xml:lang="en">The offer search by guarantee types</xsd:documentation>
                </xsd:annotation>
                <xsd:complexContent>
                    <xsd:extension base="tns:SearchOfferCriterion">
                        <xsd:sequence>
                            <xsd:element name="guaranteeType" type="xsd:string" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">Тип гарантии</xsd:documentation>
                                    <xsd:documentation xml:lang="en">Guarantee type</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="UpdateService">
                <xsd:sequence>
                    <xsd:element name="referenceId" type="xsd:string" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Номер услуги в системе клиента
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The service number in the client’s system
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="checkinHour" type="xsd:int" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Время заезда, которое хотят установить</xsd:documentation>
                            <xsd:documentation xml:lang="en">Check-in time you want to set</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="checkoutHour" type="xsd:int" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Время выезда, которое хотят установить</xsd:documentation>
                            <xsd:documentation xml:lang="en">Check-out time you want to set </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="arrivalDate" type="xsd:date" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Дата заезда, которую хотят установить</xsd:documentation>
                            <xsd:documentation xml:lang="en">Check-in date you want to set </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="departureDate" type="xsd:date" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Дата выезда, которую хотят установить</xsd:documentation>
                            <xsd:documentation xml:lang="en">Check-out date you want to set</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="addMeals" type="tns:MealIds" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Услуги питания, которые нужно добавить в услугу проживания</xsd:documentation>
                            <xsd:documentation xml:lang="en">Catering services to be added to the accommodation service </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="removeMeals" type="tns:MealIds" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Услуги питания, которые нужно удалить из услуги проживания</xsd:documentation>
                            <xsd:documentation xml:lang="en">Catering services to be removed from the accommodation service</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="sellingPrice" type="xsd:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Свайп-шкала</xsd:documentation>
                            <xsd:documentation xml:lang="en">Selling Price</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="comment" type="xsd:string" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Изменение комментария к услуге</xsd:documentation>
                            <xsd:documentation xml:lang="en">Changing a comment to a service</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="guests" type="tns:Guests" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">ФИО гостей</xsd:documentation>
                            <xsd:documentation xml:lang="en">Full name of guests</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="MealIds">
                <xsd:sequence>
                    <xsd:element name="meals" type="xsd:int" minOccurs="0" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Id питания</xsd:documentation>
                            <xsd:documentation xml:lang="en">Id of food</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="Service" abstract="true">
                <xsd:sequence>
                    <xsd:element name="referenceId" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Номер услуги в системе клиента
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The service number in the client’s system
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="extraField" type="tns:ServiceExtraField" maxOccurs="unbounded" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Список доп. полей для создания услуг
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The list of extra fields for the services
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="ServiceAccommodation">
                <xsd:complexContent>
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">
                            Информация о размещении в отеле
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">
                            An information about hotel accommodation
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:extension base="tns:Service">
                        <xsd:sequence>
                            <xsd:element name="offerCode" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Код предложения
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The offer code
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="guests" type="tns:Guests" >
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        ФИО гостей
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The name of guests
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="comment" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Комментарий к услуге
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        An information about service
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="sellingPrice" type="xsd:float">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Цена продажи
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The selling price
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="checkinHour" type="xsd:int" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Желаемое время заезда
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The desired date of check-in
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="checkoutHour" type="xsd:int" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Желаемое время выезда
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The desired date of check-out
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="preferredBedSet" type="tns:BedSet" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Предпочитаемые кровати
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Preferred Beds
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:sequence>
                                <xsd:element name="meals" type="xsd:int" maxOccurs="unbounded" minOccurs="0">
                                    <xsd:annotation>
                                        <xsd:documentation xml:lang="ru">
                                            Список заказанных услуг питания
                                        </xsd:documentation>
                                        <xsd:documentation xml:lang="en">
                                            A list of offer's booked meals
                                        </xsd:documentation>
                                    </xsd:annotation>
                                </xsd:element>
                            </xsd:sequence>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="Guests">
                <xsd:sequence>
                    <xsd:element name="guest" type="tns:Guest" minOccurs="1" maxOccurs="unbounded" />
                    <xsd:element name="children" type="tns:Child" minOccurs="0" maxOccurs="unbounded" />
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="Guest">
                <xsd:sequence>
                    <xsd:element name="firstName" type="xsd:string" />
                    <xsd:element name="lastName" type="xsd:string" />
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="HotelsWithInfo">
                <xsd:sequence>
                    <xsd:element name="hotel" type="tns:HotelWithInfo" maxOccurs="unbounded"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="HotelWithInfo">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Описание номеров
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        The hotel room description
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:complexContent>
                    <xsd:extension base="tns:Hotel">
                        <xsd:sequence>
                            <xsd:element name="categoryCertificate" type="tns:CategoryCertificate" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Cертификат, подтверждающий звездность отеля.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Hotel's star sertificate information.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="rooms" type="tns:HotelRooms"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>
            <xsd:complexType name="HotelRooms">
                <xsd:sequence>
                    <xsd:element name="room" type="tns:HotelRoom" maxOccurs="unbounded" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:element name="GetHotelOfferPricingRequest" type="tns:GetHotelOfferPricingRequest"/>
            <xsd:element name="GetHotelOfferPricingResponse" type="tns:GetHotelOfferPricingResponse"/>

            <xsd:complexType name="GetHotelOfferPricingRequest">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseRequest">
                        <xsd:sequence>
                            <xsd:element name="services" type="tns:Service" maxOccurs="unbounded"/>
                            <xsd:element name="currency" type="tns:Currency">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Валюта расчета
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The payment currency
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="GetHotelOfferPricingResponse">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseResponse">
                        <xsd:sequence>
                            <xsd:element name="services"
                                         type="tns:OrderServices"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:element name="GetOrdersChangelogRequest"
                         type="tns:GetOrdersChangelogRequest">
            </xsd:element>

            <xsd:element name="GetOrdersChangelogResponse"
                         type="tns:GetOrdersChangelogResponse">
            </xsd:element>

            <xsd:complexType name="GetOrdersChangelogRequest">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseRequest"/>
                </xsd:complexContent>
            </xsd:complexType>


            <xsd:complexType name="GetOrdersChangelogResponse">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseResponse">
                        <xsd:sequence>
                            <xsd:element type="tns:OrdersChangelogRecords" name="ordersChangelogRecords"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="OrdersChangelogRecords">
                <xsd:sequence>
                    <xsd:element name="ordersChangelogRecord" type="tns:OrdersChangelogRecord" maxOccurs="unbounded"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="OrdersChangelogRecord">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Запись об изменении в услуге
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        The record of change in order
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                    <xsd:element name="id" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Идентификатор записи об изменении
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The change record id
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="orderId" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Идентификатор заказа
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The order id
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="serviceId" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Идентификатор услуги
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The service id
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="referenceId" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Номер услуги в системе клиента
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The service number in the client system
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="serviceCreationSource" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Источник создания услуги
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The source of booking creation
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="changeSource" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Источник изменения услуги.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Source of service change.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="changeAuthor" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Автор изменения услуги.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Author of service change.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="createdAt" type="xsd:dateTime">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Дата и время изменения
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Date and time of the change
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element type="tns:ChangeList" name="changeList">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Список изменений
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The list of changes
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="OfferPolicies">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
