            </xsd:complexType>

            <xsd:complexType name="AbstractChange" abstract="true">
                <xsd:sequence>
                    <xsd:element name="element" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="GuestsChangeValue">
                <xsd:sequence>
                    <xsd:element name="guests" type="tns:Guests"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="GuestsChange">
                <xsd:complexContent>
                    <xsd:extension base="tns:AbstractChange">
                        <xsd:sequence>
                            <xsd:element name="newValue" type="tns:GuestsChangeValue"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="MealsChange">
                <xsd:complexContent>
                    <xsd:extension base="tns:AbstractChange">
                        <xsd:sequence>
                            <xsd:element name="newValue" type="tns:MealsChangeValue" />
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="MealsChangeValue">
