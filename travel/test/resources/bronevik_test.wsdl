            <xsd:complexType name="GeoLocation">
                <xsd:sequence>
                    <xsd:element name="latitude" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Широта.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Latitude.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="longitude" type="xsd:string">
