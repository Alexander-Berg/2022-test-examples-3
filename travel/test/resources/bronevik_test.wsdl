<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wsdl:definitions xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="soap.bronevik"
                  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                  name="service" targetNamespace="soap.bronevik">
    <wsdl:types>
        <xsd:schema targetNamespace="soap.bronevik">
            <xsd:simpleType name="PaymentRecipients">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Место оплаты
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        Specifies the payment scheme
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="agency"/>
                    <xsd:enumeration value="hotel"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:complexType name="Contract">
                <xsd:sequence>
                    <xsd:element name="id" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Идентификатор договора</xsd:documentation>
                            <xsd:documentation xml:lang="en">The contract id</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="number" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Номер договора</xsd:documentation>
                            <xsd:documentation xml:lang="en">The contract reference number</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="beginsAt" type="xsd:date">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Дата начала действия договора</xsd:documentation>
                            <xsd:documentation xml:lang="en">The contract start date</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="legalEntity" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Юридическое лицо</xsd:documentation>
                            <xsd:documentation xml:lang="en">Legal entity</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="Amenity">
                <xsd:sequence>
                    <xsd:element name="id" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Идентификатор доп. услуги</xsd:documentation>
                            <xsd:documentation xml:lang="en">The amenity id</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="name" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Название доп. услуги</xsd:documentation>
                            <xsd:documentation xml:lang="en">The amenity name</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="groupName" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Название группы услуг</xsd:documentation>
                            <xsd:documentation xml:lang="en">The amenity group name</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="DetailedPrice">
                <xsd:sequence>
                    <xsd:element name="price" type="xsd:float">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Стоимость
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Cost
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="vatAmount" type="xsd:float" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Сумма ндс
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The VAT amount
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="currency" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Валюта (трекхбуквенный код)
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The currency (three-letter code)
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="NamedDetailedPrice">
                <xsd:complexContent>
                    <xsd:extension base="tns:DetailedPrice">
                        <xsd:sequence>
                            <xsd:element name="name" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Наименование цены
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The price name
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>
            <xsd:complexType name="ServiceExtraField">
                <xsd:sequence>
                    <xsd:element name="name" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Название дополнительного поля для услуги
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The service extra field name
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="value" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Значение дополнительного поля
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The service extra field value
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="HotelPriceDetails">
                <xsd:sequence>
                    <xsd:element name="vatIncluded" type="xsd:boolean">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Включен ли НДС в отельную стоимость
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Whether VAT is included in the property’s price
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="ClientPriceDetails">
                <xsd:sequence>
                    <xsd:element name="vatIncluded" type="xsd:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Включен ли НДС в клиентскую стоимость
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Whether VAT is included in the client’s price
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="clientCurrency" type="tns:CurrencyPriceDetails">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Цены в валюте клиента.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Prices in client currency.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="hotelCurrency" type="tns:CurrencyPriceDetails" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Цены в валюте отеля.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Prices in hotel currency.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="CurrencyPriceDetails">
                <xsd:sequence>
                    <xsd:element name="commission" type="tns:DetailedPrice">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Информация о комиссии
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The information about commission
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="gross" type="tns:DetailedPrice">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Брутто-стоимость
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The gross-cost value
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="net" type="tns:DetailedPrice">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Нетто-стоимость
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The net-cost value
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="extra" type="tns:NamedDetailedPrice" maxOccurs="unbounded" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Дополнительные цены
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The extra prices
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="MealPriceDetails">
                <xsd:complexContent>
                    <xsd:extension base="tns:ClientPriceDetails">
                        <xsd:sequence>
                            <xsd:element name="id" type="xsd:int">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Идентификатор услуги питания
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The meal service id
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="included" type="xsd:boolean">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Включена ли услуга в предложение
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Whether the meal service is included in the offer
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>
            <xsd:complexType name="PriceDetails">
                <xsd:sequence>
                    <xsd:element name="vatApplicable" type="xsd:boolean">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Применим ли к отелю НДС
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Whether VAT is applicable
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="hotel" type="tns:HotelPriceDetails">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Детализация отельной стоимости
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Detailed hotel prices
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="client" type="tns:ClientPriceDetails">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Детализация клиентской стоимости
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The detailed client prices
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="HotelOffers">
                <xsd:sequence>
                    <xsd:element name="offer" type="tns:HotelOffer" maxOccurs="unbounded"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="AvailableAmenities">
                <xsd:sequence>
                    <xsd:element name="availableAmenity" type="tns:AvailableAmenity" minOccurs="0"
                                 maxOccurs="unbounded"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="AvailableAmenity">
                <xsd:sequence>
                    <xsd:element name="id" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Идентификатор дополнительной услуги
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The amenity id
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="price" type="xsd:float" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Стоимость доп. услуги
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The amenity price
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="currency" type="xsd:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Валюта
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Currency
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="included" type="xsd:boolean">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Включена ли услуга во все предложения отеля
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Whether the amenity is included in all properties offers
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="Meal">
                <xsd:sequence>
                    <xsd:element name="id" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Идентификатор услуги питания
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The meal service id
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="name" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Наименование услуги питания
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The meal service name
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="DescriptionDetails">
                <xsd:sequence>
                    <xsd:element name="countryId" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Идентификатор страны
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The country id
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="countryName" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Название страны
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The country name
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="distanceToCenter" type="xsd:float" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Расстояние до центра города (км)
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The distance to center from hotel (in km)
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="latitude" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Широта (географические координаты)
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The latitude (geographic coordinate)
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="longitude" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Долгота (географические координаты)
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The longitude (geographic coordinate)
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="zipCode" type="xsd:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Индекс отеля.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The hotel zip code.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="description" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Текстовое описание отеля
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The text hotel description
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="photos" type="tns:Photos" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Фотографии отеля
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The hotel's photos
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="availableAmenities"
                                 type="tns:AvailableAmenities">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Доступные в отеле доп. услуги
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Available amenities in the property
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="Photos">
                <xsd:sequence>
                    <xsd:element name="photo" type="tns:Image" maxOccurs="unbounded" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="AvailableMeal">
                <xsd:sequence>
                    <xsd:element name="id" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Идентификатор услуги питания
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The meal service id
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="included" type="xsd:boolean">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Включена ли услуга в предложение
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Whether the service is included in the offer
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="priceDetails" type="tns:ClientPriceDetails" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Детализация услуги питания.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Detailing of food service.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="VATPercent" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Ставка НДС.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                VAT percentage rate.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="AvailableMeals">
                <xsd:sequence>
                    <xsd:element name="meal" type="tns:AvailableMeal" maxOccurs="unbounded" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="EditServiceAvailableMeals">
                <xsd:complexContent>
                    <xsd:extension base="tns:AvailableMeals">
                        <xsd:sequence>
                            <xsd:element name="correctionAvailability" type="tns:MealsCorrectionAvailability" minOccurs="0" />
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="MealsCorrectionAvailability">
                <xsd:sequence>
                    <xsd:element name="adding" type="xsd:boolean"   />
                    <xsd:element name="removing" type="xsd:boolean" />
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="SkipElements">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Пропустить элементы
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        Skip elements
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                    <xsd:element name="element" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="City">
                <xsd:sequence>
                    <xsd:element name="id" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Идентификатор города</xsd:documentation>
                            <xsd:documentation xml:lang="en">The city id</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="name" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Название города</xsd:documentation>
                            <xsd:documentation xml:lang="en">The city name</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="latitude" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Широта (географические координаты)</xsd:documentation>
                            <xsd:documentation xml:lang="en">The latitude (geographic coordinate)</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="longitude" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Долгота (географические координаты)</xsd:documentation>
                            <xsd:documentation xml:lang="en">The longitude (geographic coordinate)</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="regionId" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Идентификатор региона города</xsd:documentation>
                            <xsd:documentation xml:lang="en">The city's region id</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="regionName" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Название региона города</xsd:documentation>
                            <xsd:documentation xml:lang="en">The city's region name</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="countryId" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Идентификатор страны города.</xsd:documentation>
                            <xsd:documentation xml:lang="en">The city's country id.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="countryName" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Название страны города.</xsd:documentation>
                            <xsd:documentation xml:lang="en">The city's country name.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="Country">
                <xsd:sequence>
                    <xsd:element name="id" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Идентификатор страны
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The country id
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="name" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Название страны
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The country name
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="codes" type="tns:CountryCodes">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Коды страны
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The country codes
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="CountryCodes">
                <xsd:sequence>
                    <xsd:element name="alpha2" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Код ISO3 2-буквенный
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The code ISO3 two-letter
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="alpha3" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Код ISO3 3-буквенный
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The code ISO3 three-letter
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="Credentials">
                <xsd:sequence>
                    <xsd:element name="clientKey" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Ключ клиента
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The client key
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="login" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Логин пользователя
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The client login
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="password" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Пароль пользователя
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The client password
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="FaultDetail">
                <xsd:sequence>
                    <xsd:element name="traceId" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Id запроса
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Request id
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="code" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Код Ошибки
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Error code
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="Hotel">
                <xsd:sequence>
                    <xsd:element name="id" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Идентификатор отеля
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The hotel id
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="cityId" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Идентификатор города
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The city id
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="cityName" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">
                                The city name
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="name" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Название отеля
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The hotel name
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="address" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Адрес отеля
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The hotel address
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="checkinTime" type="xsd:time">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Время заезда
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The check-in time
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="checkoutTime" type="xsd:time">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Время выезда
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The check-out time
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="descriptionDetails" type="tns:DescriptionDetails" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Описание отеля
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The hotel description
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="vatApplicable" type="xsd:boolean">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Применим ли к отелю НДС
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Returns `true` for properties located in Russian Federation and `false` for foreign
                                properties
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="hasTaxes" type="xsd:boolean">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Наличие в отеле дополнительных сборов
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Specifies whether the hotel has additional taxes
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="category" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Категория отеля (от 0 до 5, если 0 - категория не указана)
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The property star rating (0-5 stars)
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="type" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Тип отеля
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The type of the hotel
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="informationForGuest" type="tns:InformationForGuest">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Важная информация для гостей
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                An important information for guest about hotel accommodation
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="additionalInfo" type="tns:AdditionalInfo" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Дополнительная информация
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Additional info
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="paymentTerms" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Условия оплаты.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Payment terms.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="allowableCheckinTime" type="tns:TimeRange" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Допустимое время заезда.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Allowable arrival time.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="allowableCheckoutTime" type="tns:TimeRange" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Допустимое время выезда.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Allowable departure time.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="taxes" type="tns:Taxes" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Дополнительные сборы при заселении
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The additional fees at check-in
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="isHotelOnline" type="xsd:boolean">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Отель работает онлайн
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The hotel works online
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="TimeRange">
                <xsd:sequence>
                    <xsd:element name="timeFrom" type="xsd:time">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Начальное время.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Start time.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element name="timeTo" type="xsd:time">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Конечное время.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                End time.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="AdditionalInfo">
                <xsd:sequence>
                    <xsd:element name="info" type="tns:Info" maxOccurs="unbounded">
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="Info">
                <xsd:sequence>
                    <xsd:element name="name" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Название поля дополнительной информации
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Name of additional info field
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="value" type="xsd:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Значение поля дополнительной информации
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Value of additional info field
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="InformationForGuest">
                <xsd:sequence>
                    <xsd:element name="notification" type="tns:GuestNotification" minOccurs="0" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Уведомления для гостя.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Guest notifications.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="GuestNotification">
                <xsd:sequence>
                    <xsd:element name="type" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Тип уведомления.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Notification type.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="value" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Текст уведомления.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Notification text.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="CategoryCertificate">
                <xsd:sequence>
                    <xsd:element name="number" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Номер сертификата.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Certificate Number.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="endDate" type="xsd:date">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Срок действия сертификата.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Validity of the certificate.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="DailyPrice">
                <xsd:sequence>
                    <xsd:element name="date" type="xsd:date">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Дата, на которую рассчитана цена
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The date of stay
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="rate" type="tns:ClientPriceDetails" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Стоимость номера
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The price's detailing of accommodation per day
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="bookingFee" type="tns:ClientPriceDetails" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Стоимость брони в отеле
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The booking fee’s price detailing
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="earlyArrival" type="tns:ClientPriceDetails" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Стоимость раннего заезда
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The cost of early arrival
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="lateDeparture" type="tns:ClientPriceDetails" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Стоимость позднего выезда
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The cost of last departure
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="meals" type="tns:DailyPriceMeals" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Стоимость платного питания
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The cost of meal service
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="DailyPriceMeals">
                <xsd:sequence>
                    <xsd:element name="meal" type="tns:MealPriceDetails" minOccurs="0" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Детализированная информация о стоимости питания
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The detailed cost of paid meal service
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="HotelOffer">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseOffer">
                        <xsd:sequence>
                            <xsd:element name="code" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Код предложения
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The offer code
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="priceDetails" type="tns:PriceDetails">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Детализация стоимости
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The offer price details
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="taxes" type="tns:Taxes">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        @deprecated
                                        Дополнительные сборы при заселении
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        @deprecated
                                        The additional fees at check-in
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="immediateConfirmation" type="xsd:boolean">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Возможность мгновенного подтверждения
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Whether immediate confirmation is possible
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:sequence>
                                <xsd:element name="dailyPrices" type="tns:DailyPrices" minOccurs="0">
                                    <xsd:annotation>
                                        <xsd:documentation xml:lang="ru">
                                            Ежедневная детализация стоимости проживания
                                        </xsd:documentation>
                                        <xsd:documentation xml:lang="en">
                                            The detailed daily cost of accommodation
                                        </xsd:documentation>
                                    </xsd:annotation>
                                </xsd:element>
                            </xsd:sequence>
                            <xsd:sequence>
                                <xsd:element name="offerPolicies" type="tns:OfferPolicies">
                                    <xsd:annotation>
                                        <xsd:documentation xml:lang="ru">
                                            Правила предоставления предложения
                                        </xsd:documentation>
                                        <xsd:documentation xml:lang="en">
                                            A policies of granting current offer
                                        </xsd:documentation>
                                    </xsd:annotation>
                                </xsd:element>
                            </xsd:sequence>
                            <xsd:element name="deepLink" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Элемент для работы метапоисковых систем
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The element for metasearch engines
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="roomWithWindow" type="xsd:boolean">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Наличие окна в номере.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The presence of a window in the room.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>
            <xsd:simpleType name="Currency">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Код валюты (трекхбуквенный)
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        The currency (three-letter code)
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:string">
                    <xsd:length value="3"/>
                </xsd:restriction>
            </xsd:simpleType>

            <xsd:complexType name="Taxes">
                <xsd:sequence>
                    <xsd:element name="tax" type="tns:Tax" maxOccurs="unbounded" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Дополнительные сборы при заселении
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The additional fees at check-in
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="PingRequest">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseRequest">
                        <xsd:sequence>
                            <xsd:element name="data" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">Передаваемые в запросе данные</xsd:documentation>
                                    <xsd:documentation xml:lang="en">The data that passed in request and returned in
                                        response
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>
            <xsd:complexType name="PingResponse">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseResponse">
                        <xsd:sequence>
                            <xsd:element name="data" type="xsd:string"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>
            <xsd:complexType name="BaseRequest" abstract="true">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Базовая структура запроса</xsd:documentation>
                    <xsd:documentation xml:lang="en">The base structure of request</xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                    <xsd:element name="credentials" type="tns:Credentials">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Идентификационные данные</xsd:documentation>
                            <xsd:documentation xml:lang="en">The identification data</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="language">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Язык, используемый в ответе</xsd:documentation>
                            <xsd:documentation xml:lang="en">The response language</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value="ru"/>
                                <xsd:enumeration value="en"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="BaseResponse" abstract="true">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Базовая структура ответа</xsd:documentation>
                    <xsd:documentation xml:lang="en">The base structure of response</xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                    <xsd:element name="traceId" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:element name="Fault" type="tns:FaultDetail"/>
            <xsd:element name="GetAmenitiesRequest" type="tns:GetAmenitiesRequest"/>
            <xsd:element name="GetAmenitiesResponse" type="tns:GetAmenitiesResponse"/>
            <xsd:element name="PingRequest" type="tns:PingRequest"/>
            <xsd:element name="PingResponse" type="tns:PingResponse"/>
            <xsd:element name="GetMealsRequest" type="tns:GetMealsRequest"/>
            <xsd:element name="GetMealsResponse" type="tns:GetMealsResponse"/>
            <xsd:element name="GetCountriesRequest" type="tns:GetCountriesRequest"/>
            <xsd:element name="GetCountriesResponse" type="tns:GetCountriesResponse"/>
            <xsd:element name="GetOrderInvoicesRequest" type="tns:GetOrderInvoicesRequest"/>
            <xsd:element name="GetOrderInvoicesResponse" type="tns:GetOrderInvoicesResponse"/>
            <xsd:element name="GetServicePricingRequest" type="tns:GetServicePricingRequest"/>
            <xsd:element name="GetServicePricingResponse" type="tns:GetServicePricingResponse" />
            <xsd:element name="GetServiceAvailableCorrectionsRequest" type="tns:GetServiceAvailableCorrectionsRequest" />
            <xsd:element name="GetServiceAvailableCorrectionsResponse" type="tns:GetServiceAvailableCorrectionsResponse"/>
            <xsd:complexType name="GetServiceAvailableCorrectionsRequest">
				<xsd:annotation>
                    <xsd:documentation xml:lang="ru">Запрос на получение доступных коррекций и информацию о них</xsd:documentation>
                    <xsd:documentation xml:lang="en">Request to get available corrections and information about it</xsd:documentation>
                </xsd:annotation>
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseRequest">
                        <xsd:sequence>
                            <xsd:element name="serviceId" type="xsd:int">
								<xsd:annotation>
									<xsd:documentation xml:lang="ru">ID услуги, по которой нужно узнать информацию о коррекции</xsd:documentation>
									<xsd:documentation xml:lang="en">Service ID for which you need to get an information about corrections</xsd:documentation>
								</xsd:annotation>
                            </xsd:element>
                            <xsd:element name="availableCorrectionTypes" type="tns:AvailableCorrectionTypes" />
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>
            <xsd:complexType name="GetServiceAvailableCorrectionsResponse">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseResponse">
                        <xsd:sequence>
                            <xsd:element name="service" type="tns:EditService" />
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>
            <xsd:complexType name="GetAmenitiesRequest">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseRequest"/>
                </xsd:complexContent>
            </xsd:complexType>
            <xsd:complexType name="GetAmenitiesResponse">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseResponse">
                        <xsd:sequence>
