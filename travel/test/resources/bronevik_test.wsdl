            <xsd:complexType name="AddElements">
                <xsd:sequence>
                    <xsd:element name="addElement" type="xsd:string" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Дополнительная информация в ответе.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Additional information in response.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="HotelsWithCheapestOffer">
                <xsd:sequence>
                    <xsd:element name="hotel" type="tns:HotelWithCheapestOffer" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Отель с самым дешевым оффером.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Hotel with the cheapest offer.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
