                            <xsd:documentation xml:lang="en">
                                Information about VAT at the hotel.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="HotelGeo">
                <xsd:sequence>
                    <xsd:element name="cityId" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Идентификатор города.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                City ID.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="cityName" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Название города.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                City name.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="address" type="xsd:string">
