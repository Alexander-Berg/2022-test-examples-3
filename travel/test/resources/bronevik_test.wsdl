            <xsd:element name="GetServiceMessagesRequest"
                         type="tns:GetServiceMessagesRequest">
            </xsd:element>

            <xsd:element name="GetServiceMessagesResponse"
                         type="tns:GetServiceMessagesResponse">
            </xsd:element>

            <xsd:complexType name="GetServiceMessagesRequest">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseRequest">
                        <xsd:sequence>
                            <xsd:element name="serviceId" type="xsd:int" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Идентификатор услуги, по которой будет поиск комментариев.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The identifier of the service that will be searched for comments.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="serviceMessages" type="tns:ServiceMessages" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Идентификаторы комментариев, по которым будет поиск комментариев.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Comment identifiers for which comments will be searched.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="GetServiceMessagesResponse">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseResponse">
                        <xsd:sequence>
                            <xsd:element name="messages" type="tns:Messages">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Найденные комментарии.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Founded comments.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="RecordIds">
                <xsd:sequence>
                    <xsd:element name="id" type="xsd:int" maxOccurs="unbounded"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="Messages">
                <xsd:sequence>
                    <xsd:element name="message" type="tns:Message" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Массив найденных комментариев.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Array of founded comments.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="Message">
                <xsd:sequence>
                    <xsd:element name="id" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Идентификатор комментария.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Сomment identifier.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="serviceId" type="xsd:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Идентификатор услуги, к которой относится комментарий.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The identifier of the service to which the comment relates.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="senderType" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Тип отправителя: менеджер или клиент.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Type of sender: manager or client.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="date" type="xsd:dateTime">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Дата и время отправки комментария.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Date and time of the comment.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="senderName" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Имя отправителя комментария.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The name of the sender of the comment.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="text" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Текст комментария.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Comment text.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="ServiceMessages">
                <xsd:sequence>
                    <xsd:element name="id" type="xsd:int" maxOccurs="100">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Массив идентификаторов комментариев, по которым будет поиск комментариев.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                Array of comment identifiers for which comments will be searched.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>


            <xsd:element name="SendServiceMessageRequest"
                         type="tns:SendServiceMessageRequest">
            </xsd:element>

            <xsd:element name="SendServiceMessageResponse"
                         type="tns:SendServiceMessageResponse">
            </xsd:element>

            <xsd:complexType name="SendServiceMessageRequest">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseRequest">
                        <xsd:sequence>
                            <xsd:element name="serviceId" type="xsd:int">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Идентификатор услуги, к которой будет написан комментарий.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Service identifier to which the comment will be written.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="text" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Текст комментария.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Comment text.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="SendServiceMessageResponse">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseResponse">
                        <xsd:sequence>
                            <xsd:element name="id" type="xsd:int">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Идентификатор добавленного комментария.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The identifier of the added comment.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>


            <xsd:element name="SearchHotelAvailabilityRequest"
                         type="tns:SearchHotelAvailabilityRequest">
            </xsd:element>

            <xsd:element name="SearchHotelAvailabilityResponse"
                         type="tns:SearchHotelAvailabilityResponse">
            </xsd:element>

            <xsd:complexType name="SearchHotelAvailabilityRequest">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseRequest">
                        <xsd:sequence>
                            <xsd:element name="cityId" type="xsd:int" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Идентификатор города, по которому будет осуществлен поиск.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The identifier of the city that will be searched.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="hotelIds" type="tns:HotelIds" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Идентификаторы отелей, по которым будет осуществлен поиск.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The identifiers of the hotels that will be searched.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="geolocation" type="tns:GeoLocation" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Координаты и радиус для поиска.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Coordinates and radius for search.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="checkInDate" type="xsd:date">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Дата заезда.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Arrival date.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="checkOutDate" type="xsd:date">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Дата выезда.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Departure date.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="currency" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Валюта (трекхбуквенный код)
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The currency (three-letter code)
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="searchCriteria"
                                         type="tns:SearchOfferCriterion" maxOccurs="unbounded"
                                         minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Критерии поиска.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The search criteria.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="addElements" type="tns:AddElements" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="SearchHotelAvailabilityResponse">
                <xsd:complexContent>
                    <xsd:extension base="tns:BaseResponse">
                        <xsd:sequence>
                            <xsd:element name="hotels" type="tns:HotelsWithCheapestOffer">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Отели с самым дешевым оффером.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Hotels with the cheapest offer.
