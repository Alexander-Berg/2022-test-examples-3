{
    "extends": "../../.eslintrc.js",
    "rules": {
        "@typescript-eslint/no-use-before-define": [
            "error",
            {
                "functions": false,
                "classes": false,
                "typedefs": false
            }
        ],
        "comma-dangle": ["off"],
        "import/order": [
            "error",
            {
                "groups": ["builtin", "external", "internal", "parent", "sibling", "index"]
            }
        ]
    }
}
