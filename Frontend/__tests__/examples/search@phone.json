                            {
                                "block": "health-serp-group",
                                "title": "Энциклопедия",
                                "titleUrl": "/health/search/encyclopedia?text=%D0%B0%D1%81%D1%82%D0%BC%D0%B0&parent-reqid=1569329008799149-13784068992891676100040-man1-2919-man-shared-app-host--038-14503",
